<h1><%=@artist.name%></h1>
<br>
<h3>Artist Bio:</h3>
<p><%=@artist.bio%></p>
<br>
<h3>Songs:</h3>
<ul>
<%@artist.songs.each do |song|%>
<li><%=link_to song.name, song%></li>
<%end%>
</ul>
<br>
<h3>Genres:</h3>
<ul>
<%@artist.genres.each do |genre|%>
  <li><%=link_to genre.name, genre%></li>
<%end%>
</ul>


